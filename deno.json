{
  "imports": {
    "@dest/": "./_lib/",
    "@utils/": "./utils/",
    "hono": "jsr:@hono/hono@^4.5.9",
    "hono/jsx": "jsr:@hono/hono@^4.5.9/jsx",
    "zod": "https://deno.land/x/zod@v3.23.8/mod.ts",
    "@std/assert": "jsr:@std/assert@1.0.3",
    "@std/testing": "jsr:@std/testing@1.0.1",
    "@std/cli": "jsr:@std/cli@1.0.6"
  },
  "tasks": {
    "start": "deno run -A --unstable-kv main.ts",
    "test:unit": "deno test -A --unstable-kv ./__tests__/unit",
    "test:e2e": "deno test -A --unstable-kv --fail-fast ./__tests__/e2e",
    "users": "deno run -A --unstable-kv scripts/users.ts"
  },
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  }
}
